<h1>Details</h1>
<div class="row justify-content-center d-flex"></div>
<div class="col-md-4 mb-3">
    <div class="card">
        <div class="card-body">
            <h3 class="card-title">{{title}}</h3>
            <p class="card-text"> Genre:{{genre}}</p>
            <p class="card-text">Plot:{{plot}}</p>
            {{#each cast}}
            <p class="card-text">Actor/Actress: {{name}}</p>
            {{/each}}
        </div>
    </div>
</div>
</div>